    "i18next": "^20.1.0",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "mobx-sync": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.12",
    "react-is": "^17.0.2",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "serializr": "^2.0.5",
    "styled-components": "^5.2.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@*",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.1",
    "workbox-background-sync": "^6.1.2",
    "workbox-broadcast-update": "^6.1.2",
    "workbox-cacheable-response": "^6.1.2",
    "workbox-core": "^6.1.2",
    "workbox-expiration": "^6.1.2",
    "workbox-google-analytics": "^6.1.2",
    "workbox-navigation-preload": "^6.1.2",
    "workbox-precaching": "^6.1.2",
    "workbox-range-requests": "^6.1.2",
    "workbox-routing": "^6.1.2",
    "workbox-strategies": "^6.1.2",
    "workbox-streams": "^6.1.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint --ext js ./src/**/*.{js,ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "alphabetize": {
            "order": "desc",
            "caseInsensitive": true
          },
          "pathGroups": [
            {
              "pattern": "^[a-zA-Z]",
              "group": "builtin",
              "position": "after"
            },
            {
              "pattern": "@/**",
              "group": "internal",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ],
          "groups": [
            [
              "builtin",
              "external"
            ],
            "internal",
            "parent",
            "sibling"
          ],
          "newlines-between": "always"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "autoprefixer": "^9.0.0",
    "eslint": "^7.23.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "postcss": "^7",
    "typescript": "^4.2.3"
  }
}
